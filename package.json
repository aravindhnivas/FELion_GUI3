  "repository": {
    "type": "git",
    "url": "https://github.com/aravindhnivas/FELion_GUI3.git"
  },
  "developer": true,
  "main": "main.js",
  "devDependencies": {
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "bulma": "^0.8.2",
    "concurrently": "^4.1.0",
    "electron": "^8.3.3",
    "electron-packager": "^14.2.1",
    "electron-winstaller": "^4.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.32.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-postcss": "^2.9.0",
    "rollup-plugin-svelte": "^5.2.2",
    "rollup-plugin-terser": "^5.3.0",
    "sirv-cli": "^0.4.6",
    "svelma": "^0.3.2",
    "svelte": "^3.23.2",
    "svelte-material-ui": "^1.0.0-beta.21",
    "svelte-preprocess": "^3.7.4"
  },
  "scripts": {
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "concurrently \"run-p start:dev autobuild\" \"electron .\"",
    "start": "sirv public",
    "start:dev": "sirv public --dev",
    "mc": "electron-packager . --overwrite --platform=darwin --arch=x64 --out=../release-builds --darwinDarkModeSupport=true",
    "w": "electron-packager . --overwrite --platform=win32 --arch=x64 --out=../release-builds"
  },
  "dependencies": {
    "adm-zip": "^0.4.14",
    "directory-tree": "^2.2.4",
    "recursive-copy": "^2.0.10"
  }
}
