    "type": "git",
    "url": "https://github.com/aravindhnivas/FELion_GUI3.git"
  },
  "description": "FELion Spectrum Analyser for FELIX data",
  "main": "packages/main/dist/main.cjs",
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@formkit/auto-animate": "^1.0.0-beta.5",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^1.3.1",
    "@types/lodash-es": "^4.17.6",
    "@types/plotly.js": "^2.12.9",
    "@types/plotly.js-basic-dist": "^1.54.1",
    "@types/winbox": "^0.2.1",
    "@zerodevx/svelte-toast": "^0.8.2",
    "animate.css": "^4.1.1",
    "buffer": "^6.0.3",
    "bulma": "^0.9.4",
    "bulma-extensions": "^6.2.7",
    "concurrently": "^7.6.0",
    "cooltipz-css": "^2.1.1",
    "cross-env": "^7.0.3",
    "dygraphs": "^2.1.0",
    "electron": "^21.3.1",
    "electron-builder": "^23.6.0",
    "eslint": "^8.28.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "get-port": "^6.1.2",
    "gridjs": "^5.1.0",
    "gridjs-svelte": "^2.1.1",
    "interactjs": "^1.10.17",
    "lodash-es": "^4.17.21",
    "material-symbols": "^0.4.0",
    "prettier-plugin-svelte": "^2.8.1",
    "regression": "^2.0.1",
    "sass": "^1.56.1",
    "svelte": "^3.53.1",
    "svelte-check": "^2.9.2",
    "svelte-clipboard": "^1.0.0",
    "svelte-flatlist": "^0.2.6",
    "svelte-markdown": "^0.2.3",
    "svelte-material-ui": "^6.2.0",
    "svelte-preprocess": "^4.10.7",
    "ts-try": "^0.2.0",
    "tsx": "^3.12.1",
    "unplugin-auto-import": "^0.12.0",
    "vite": "^3.2.4",
    "vite-plugin-inspect": "^0.7.9",
    "winbox": "^0.2.6",
    "yaml": "^2.1.3"
  },
  "scripts": {
    "start": "electron .",
    "build": "npm run update:vendor && npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "dev": "tsx scripts/watch.ts",
    "update:vendor": "tsx scripts/update-electron-vendors.ts",
    "svelte-check": "svelte-check",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config electron-builder.config.js --dir",
    "pack": "electron-builder build --config electron-builder.config.js --win -p never",
    "publish": "electron-builder build --config electron-builder.config.js --win -p always",
    "fix": "npx eslint --fix \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "format": "npx prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "py": "node scripts/build.py.js",
    "buildall": "npm run py && npm run compile"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^5.3.0",
    "fs-extra": "^11.0.0",
    "plotly.js-basic-dist": "^2.16.3"
  }
}
