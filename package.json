{
  "name": "FELion_GUI3",
  "version": "3.4.0",
  "license": "MIT",
  "author": "A.N.Marimuthu",
  "repository": {
    "type": "git",
    "url": "https://github.com/aravindhnivas/FELion_GUI3.git"
  },
  "description": "FELion Spectrum Analyser for FELIX data",
  "main": "main.js",
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-yaml": "^3.1.0",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "bulma": "^0.9.3",
    "concurrently": "^6.3.0",
    "electron": "^15.2.0",
    "electron-builder": "^22.13.1",
    "postcss": "^8.3.6",
    "rollup": "^2.55.1",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-polyfill-node": "^0.7.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "^1.35.1",
    "svelma": "^0.3.2",
    "svelte": "^3.42.1",
    "svelte-check": "^2.2.3",
    "svelte-material-ui": "^1.0.0",
    "svelte-preprocess": "^4.7.4"
  },
  "scripts": {
    "check-build": "npm run svelte-check && rollup -c",
    "build": "rollup -c",
    "autobuild": "rollup -c -w",
    "dev": "concurrently \"npm run autobuild\" \"electron .\"",
    "start": "electron .",
    "svelte-check": "svelte-check",
    "publish": "electron-builder --win -p always",
    "pack": "electron-builder --win -p never",
    "bpublish": "npm run build && electron-builder --win -p always"
  },
  "dependencies": {
    "bulma-extensions": "^6.2.7",
    "cooltipz-css": "^1.6.8",
    "electron-log": "^4.4.1",
    "electron-updater": "^4.3.9",
    "fs-extra": "^10.0.0",
    "source-map-support": "^0.5.20",
    "yaml": "2.0.0-6"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "aravindhnivas",
        "repo": "FELion_GUI3"
      }
    ],
    "win": {
      "target": "nsis",
      "icon": "./static/assets/logo/win/icon.ico",
      "asar": false
    },
    "nsis": {
      "oneClick": false
    }
  }
}
