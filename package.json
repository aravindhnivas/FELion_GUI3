{
  "name": "FELion_GUI3",
  "version": "3.8.0-beta1",
  "license": "MIT",
  "author": "A.N.Marimuthu",
  "repository": {
    "type": "git",
    "url": "https://github.com/aravindhnivas/FELion_GUI3.git"
  },
  "description": "FELion Spectrum Analyser for FELIX data",
  "main": "packages/main/dist/main.cjs",
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.9",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^1.0.1",
    "@types/lodash-es": "^4.17.6",
    "@types/plotly.js": "^2.12.2",
    "@types/plotly.js-basic-dist": "^1.54.0",
    "@types/winbox": "^0.2.1",
    "@zerodevx/svelte-toast": "^0.7.2",
    "animate.css": "^4.1.1",
    "buffer": "^6.0.3",
    "bulma": "^0.9.4",
    "bulma-extensions": "^6.2.7",
    "concurrently": "^7.3.0",
    "cooltipz-css": "^2.1.0",
    "cross-env": "^7.0.3",
    "dygraphs": "^2.1.0",
    "electron": "^19.0.10",
    "electron-builder": "^23.3.3",
    "eslint": "^8.20.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "get-port": "^6.1.2",
    "gridjs": "^5.0.2",
    "gridjs-svelte": "^2.1.1",
    "interactjs": "^1.10.17",
    "lodash-es": "^4.17.21",
    "material-symbols": "^0.2.7",
    "prettier-plugin-svelte": "^2.7.0",
    "regression": "^2.0.1",
    "sass": "^1.54.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-clipboard": "^1.0.0",
    "svelte-flatlist": "^0.2.6",
    "svelte-markdown": "^0.2.2",
    "svelte-material-ui": "^6.0.0",
    "svelte-preprocess": "^4.10.7",
    "svelte-sortable-list": "^1.1.0",
    "ts-try": "^0.2.0",
    "vite": "^3.0.4",
    "winbox": "^0.2.1",
    "yaml": "^2.1.1"
  },
  "scripts": {
    "start": "electron .",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "cd ./packages/main && vite build",
    "build:preload": "cd ./packages/preload && vite build",
    "build:renderer": "cd ./packages/renderer && vite build",
    "dev": "tsx scripts/watch.ts",
    "svelte-check": "svelte-check",
    "precompile": "cross-env MODE=production npm run build",
    "compile": "electron-builder build --config electron-builder.config.js --dir",
    "pack": "electron-builder build --config electron-builder.config.js --win -p never",
    "publish": "electron-builder build --config electron-builder.config.js --win -p always",
    "fix": "npx eslint --fix \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "format": "npx prettier --write \"{,!(node_modules|public)/**/}*.{js,svelte}\"",
    "py": "node scripts/build.py.js",
    "buildall": "npm run py && npm run compile"
  },
  "dependencies": {
    "electron-log": "^4.4.8",
    "electron-store": "^8.1.0",
    "electron-unhandled": "^4.0.1",
    "electron-updater": "^5.2.1",
    "fs-extra": "^10.1.0",
    "plotly.js-basic-dist": "^2.13.3"
  }
}
